<?xml version="1.0" encoding="utf-8"?>
<XmlTemplateCategoryInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="operators">
  <Template Name="or">
    <Expansion Priority="0">
      <Body>|| </Body>
      <Context />
      <ContextString>[LeftTextMatchesRegEx("\s*if\s*.*")]</ContextString>
    </Expansion>
  </Template>
  <Template Name="and">
    <Expansion Priority="0">
      <Body>&amp;&amp; </Body>
      <Context />
      <ContextString>[LeftTextMatchesRegEx("\s*if\s*.*")]</ContextString>
    </Expansion>
  </Template>
</XmlTemplateCategoryInfo>